<style>
table, th, td {
  border: 1px solid black;
}
</style>
{{#if iNotLoaded}}
<h2>These {{iNotLoaded}} didn't load:</h2>
<form>
<table style="width: 600px">
<tr>
    <th>Given Name</th>
    <th>Family Name</th>
    <th>Email</th>
</tr>
    {{#each aoNotLoaded}}
    <tr>
        <td>{{this.GivenName}}</td>
        <td>{{this.FamilyName}}</td>
        <td>{{this.E-mail1-Value}}</td>
    </tr>
    {{/each}}
</table>
</form>
{{else}}
<h2> All loaded!</h2>
{{/if}}
<br>
<form method="get" action="/searchPage">
    <button type="submit">Return to search page</button>
</form>

